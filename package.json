  "scripts": {
    "dev:server": "tsx watch src/server.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "dev:review-worker": "tsx watch src/workers/review-orchestrator.ts",
    "dev:indexer": "tsx watch src/workers/indexer.ts",
